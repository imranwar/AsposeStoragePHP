{
    "description": 	"Aspose.Storage Cloud SDK for PHP lets users use Aspose Cloud Storage APIs in their PHP apps.",
    "name": 		"imranwar/asposestoragephp",
    "type": 		"library",
    "keywords" : 	["aspose", "php", "aspose cloud", "storage"],
	"homepage": 	"https://github.com/imranwar/AsposeStoragePHP",
	"license": 		"MIT",
	"authors": [
        {
            "name": 	"Imran Anwar",
            "email": 	"imranwar@gmail.com"
        }
    ],
    "require": {
        "php": 		">=5.3.2",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-0": {
            "Imranwar": "SDKs/Aspose.Storage_Cloud_SDK_For_PHP/src/"
        },
		"exclude-from-classmap": ["SDKs/Aspose.Storage_Cloud_SDK_For_CSharp/*","SDKs/Aspose.Storage_Cloud_SDK_For_Java/*","SDKs/Aspose.Storage_Cloud_SDK_For_NodeJS/*","SDKs/Aspose.Storage_Cloud_SDK_For_Python/*"]
		
    },
    "autoload-dev": {
        "psr-4": {
			"Imranwar\\Test\\": "test/"
		}
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "imranwar/asposestoragephp",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/imranwar/AsposeStoragePHP/releases/download/v1.0.0/Aspose.Storage_Cloud_SDK_For_PHP-v1.0.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/imranwar/AsposeStoragePHP",
                    "type": "git",
                    "reference": "releases/tag/v1.0.0"
                }
            }
        }
    ]
}
